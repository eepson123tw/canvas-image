{
  "name": "@eepson123tw/canvas-meme-image",
  "version": "0.0.3",
  "description": "A Vue component for displaying canvas images.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/canvas-image.umd.js",
  "module": "dist/canvas-image.es.js",
  "exports": {
    ".": {
      "import": "./dist/canvas-image.es.js",
      "require": "./dist/canvas-image.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/node": "^20.14.12",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "path": "^0.12.7",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vue-tsc": "^2.0.24"
  },
  "keywords": [
    "canvas",
    "vue.js",
    "component",
    "vue",
    "meme",
    "text to image"
  ],
  "author": "eepson123tw",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@eepson123tw"
  }
}
