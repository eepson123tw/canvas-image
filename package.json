{
  "name": "@eepson123tw/canvas-meme-image",
  "version": "0.0.14",
  "description": "A Vue component for displaying canvas images.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/canvas-image.umd.js",
  "module": "dist/canvas-image.es.js",
  "exports": {
    ".": {
      "import": "./dist/canvas-image.es.js",
      "require": "./dist/canvas-image.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && pnpm run generate:types",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "prepare": "husky",
    "release": "./release.sh",
    "precommit": "node scripts/verify-commit.js",
    "generate:types": "vue-tsc --emitDeclarationOnly"
  },
  "types": "./dist/src/index.d.ts",
  "dependencies": {
    "@types/node": "^20.14.12",
    "enquirer": "^2.4.1",
    "pico": "^0.1.17",
    "picocolors": "^1.0.1",
    "semver": "^7.6.3",
    "vite-plugin-dts": "4.0.0-beta.1",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "husky": "^9.1.3",
    "path": "^0.12.7",
    "standard-version": "^9.5.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vue-tsc": "^2.0.24"
  },
  "keywords": [
    "canvas",
    "vue.js",
    "component",
    "vue",
    "meme",
    "text to image"
  ],
  "author": "eepson123tw",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@eepson123tw"
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  }
}
